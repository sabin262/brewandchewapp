<h1 align="center">Food Menu</h1>
<div class="container">
	<table class="table table-dark">
	  <thead>
	    <tr>
	      <th scope="col">S.No</th>
	      <th scope="col">ID</th>
	      <th scope="col">Name</th>
	      <th scope="col">Price</th>
	      <th scope="col">Category</th>
	    </tr>
	  </thead>
	  <tbody>
	  	<% id=0 %>
	  	<% @food.each do |food| %>
	    <tr>
	      <th scope="row"><%= id = id + 1 %></th>
	      <td><%= food.id %></td>
	      <td><%= link_to food.name, edit_food_path(food) %></td>
	      <td><%= food.price %></td>
	      <td><%= food.category %></td>
	    </tr>
	    <% end %>
	  </tbody>
	</table>
</div>
<div class="container">
	<%= link_to "Add Food" , new_food_path, class: "btn btn-primary btn-lg"%>
</div>
